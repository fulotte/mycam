{
  "api_name": "mycam-api",
  "description": "MyCam Camera Monitor API",
  "stage_name": "prod",
  "apis": [
    {
      "name": "upload-image",
      "path": "/api/v1/images/upload",
      "method": "POST",
      "function": "upload-handler"
    },
    {
      "name": "list-images",
      "path": "/api/v1/images/list",
      "method": "GET",
      "function": "image-query"
    },
    {
      "name": "device-token",
      "path": "/api/v1/device/token",
      "method": "POST",
      "function": "device-manager"
    },
    {
      "name": "device-register",
      "path": "/api/v1/device/register",
      "method": "POST",
      "function": "device-manager"
    },
    {
      "name": "device-config-get",
      "path": "/api/v1/device/{device_id}/config",
      "method": "GET",
      "function": "device-config"
    },
    {
      "name": "device-config-update",
      "path": "/api/v1/device/{device_id}/config",
      "method": "POST",
      "function": "device-config"
    },
    {
      "name": "device-config-patch",
      "path": "/api/v1/device/{device_id}/config",
      "method": "PATCH",
      "function": "device-config"
    }
  ]
}
